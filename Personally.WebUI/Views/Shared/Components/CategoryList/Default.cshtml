@model CategoryListViewModel

<div class="card my-4">
    <h5 class="card-header">Categories</h5>
    <div class="card-body">
        <div class="row">
            @foreach (var item in Model.Categories)
            {
                <div class="col-lg-6">
                    <ul class="list-unstyled mb-0">
                        <li>
                            <a href="#">@item.Title</a>
                        </li>
                    </ul>
                </div>}

        </div>
    </div>
</div>
